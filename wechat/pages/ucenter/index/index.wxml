
<view class="view_contain"> 
  <!-- 第一部分 -->
  <view class="view_1">
    <view class="view_image_text" bindtap="goLogin">
      <image class="image_radius" src="{{userInfo.avatarUrl}}" />
      <text >{{userInfo.nickName}}</text>
    </view>
  </view>
  <view class="view_2" wx:for="{{elements}}" wx:key="index">
    <text bindtap="handletap" data-idx="{{index}}">{{item.title}}</text>
    <view class="hr"></view>
  </view>
</view>

